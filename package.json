{
  "name": "tuxtql",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "relay": "relay-compiler",
    "watch": "relay-compiler --watch",
    "dev": "yarn relay && next dev",
    "build": "yarn relay && next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.9.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.1",
    "@mui/material": "^5.6.1",
    "clsx": "^1.1.1",
    "graphql-ws": "^5.7.0",
    "next": "^12.1.5",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-relay": "^13.2.0",
    "relay-nextjs": "^0.8.0",
    "relay-runtime": "^13.2.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.24",
    "@types/react": "^18.0.4",
    "@types/react-dom": "^18.0.0",
    "@types/react-relay": "^13",
    "@types/relay-runtime": "^13",
    "@types/ws": "^8",
    "eslint": "^8.13.0",
    "eslint-config-next": "^12.1.5",
    "graphql": "^16.3.0",
    "prettier": "^2.6.2",
    "relay-compiler": "^13.2.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3",
    "ws": "^8.5.0"
  },
  "browserslist": [
    ">= 1%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "relay": {
    "src": "./",
    "schema": "./schema/schema.graphql",
    "artifactDirectory": "./__generated__",
    "excludes": [
      "**/node_modules/**",
      "**/__mocks__/**",
      "**/__generated__/**"
    ],
    "language": "typescript"
  },
  "packageManager": "yarn@3.2.0"
}
